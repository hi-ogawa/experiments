{
	"include": ["src"],
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "vue",
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"strict": true,
		"noEmit": true,
		"verbatimModuleSyntax": true,
		"moduleResolution": "Bundler",
		"module": "ESNext",
		"target": "ESNext",
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"types": []
	}
}
