{
	"include": ["src", "vite.config.ts"],
	"compilerOptions": {
		"jsx": "react-jsx",
		"skipLibCheck": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"strict": true,
		"noEmit": true,
		"verbatimModuleSyntax": true,
		"moduleResolution": "Bundler",
		"module": "ESNext",
		"target": "ESNext",
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"types": ["vite/client", "react/experimental"]
	}
}
