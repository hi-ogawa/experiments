{
	"exclude": ["node_modules"],
	"compilerOptions": {
		"checkJs": true,
		"allowJs": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"verbatimModuleSyntax": true,
		"strict": true,
		"noEmit": true,
		"moduleResolution": "Bundler",
		"module": "ESNext",
		"target": "ESNext",
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"types": ["react/experimental"],
		"jsx": "react-jsx"
	}
}
